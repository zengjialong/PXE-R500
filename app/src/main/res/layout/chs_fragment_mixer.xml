<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:wheelview="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.chs.mt.pxe_r500.fragment.MixerFragment">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            >
            <!-- 通道选择 -->
            <com.chs.mt.pxe_r500.tools.wheel.WheelView
                android:id="@+id/id_output_va_wheelview"
                android:layout_width="match_parent"
                android:layout_height="@dimen/space_35"
                android:layout_marginBottom="10dp"
                android:layout_gravity="center"
                android:background="@drawable/channel_bg"
                wheelview:fontColor="@color/text_color_TopBarChannelSel"
                wheelview:fontSize="@dimen/text_size_TopBarChannelSel"
                />

            <LinearLayout
                android:layout_gravity="center"
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="@dimen/space_30"
                android:layout_marginTop="@dimen/space_10"
                android:layout_marginBottom="@dimen/space_10"
                android:orientation="horizontal"
                >
                <LinearLayout
                    android:id="@+id/ly_input"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:layout_width="@dimen/space_200"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    >
                    <Button
                        android:id="@+id/id_b_passive"
                        android:layout_width="@dimen/space_100"
                        android:layout_height="@dimen/space_30"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:textAllCaps="false"
                        android:background="@color/transparent"
                        android:text="@string/Passive_input"
                        android:textColor="@color/eq_page_text_eq_set_normal"
                        android:textSize="13sp"/>
                    <Button
                        android:id="@+id/id_b_active"
                        android:layout_width="@dimen/space_100"
                        android:layout_height="@dimen/space_30"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:textAllCaps="false"
                        android:background="@color/transparent"
                        android:text="@string/Active_input"
                        android:textColor="@color/eq_page_text_eq_set_normal"
                        android:textSize="13sp"/>
                </LinearLayout>


            </LinearLayout>

            <com.chs.mt.pxe_r500.tools.MScrollView
                android:layout_weight="1.0"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:fillViewport="true"
                android:scrollbars="none"
                android:layout_gravity="center">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fillViewport="true"
                    android:orientation="vertical">





                    <com.chs.mt.pxe_r500.viewItem.V_MixerItem
                        android:id="@+id/id_mixer_0"
                        android:layout_width="match_parent"
                        android:layout_height="95dp"/>
                    <com.chs.mt.pxe_r500.viewItem.V_MixerItem
                        android:id="@+id/id_mixer_1"
                        android:layout_width="match_parent"
                        android:layout_height="95dp"/>
                    <com.chs.mt.pxe_r500.viewItem.V_MixerItem
                        android:id="@+id/id_mixer_2"
                        android:layout_width="match_parent"
                        android:layout_height="95dp"/>
                    <com.chs.mt.pxe_r500.viewItem.V_MixerItem
                        android:id="@+id/id_mixer_3"
                        android:layout_width="match_parent"
                        android:layout_height="95dp"/>
                    <com.chs.mt.pxe_r500.viewItem.V_MixerItem
                        android:id="@+id/id_mixer_4"
                        android:layout_width="match_parent"

                        android:layout_height="95dp"/>
                    <com.chs.mt.pxe_r500.viewItem.V_MixerItem
                        android:id="@+id/id_mixer_5"
                        android:layout_width="match_parent"
                        android:layout_height="95dp"/>
                    <com.chs.mt.pxe_r500.viewItem.V_MixerItem
                        android:id="@+id/id_mixer_6"
                        android:layout_width="match_parent"
                        android:layout_height="95dp"/>
                    <com.chs.mt.pxe_r500.viewItem.V_MixerItem
                        android:id="@+id/id_mixer_7"
                        android:layout_width="match_parent"
                        android:layout_height="95dp"/>
                    <com.chs.mt.pxe_r500.viewItem.V_MixerItem
                        android:id="@+id/id_mixer_8"
                        android:layout_width="match_parent"


                        android:layout_height="95dp"/>
                    <com.chs.mt.pxe_r500.viewItem.V_MixerItem
                        android:id="@+id/id_mixer_9"
                        android:layout_width="match_parent"
                        android:layout_height="95dp"/>
                    <com.chs.mt.pxe_r500.viewItem.V_MixerItem
                        android:id="@+id/id_mixer_10"
                        android:layout_width="match_parent"
                        android:layout_height="95dp"/>
                    <com.chs.mt.pxe_r500.viewItem.V_MixerItem
                        android:id="@+id/id_mixer_11"
                        android:layout_width="match_parent"
                        android:layout_height="95dp"/>
                    <com.chs.mt.pxe_r500.viewItem.V_MixerItem
                        android:id="@+id/id_mixer_12"
                        android:layout_width="match_parent"
                        android:layout_height="95dp"/>
                    <com.chs.mt.pxe_r500.viewItem.V_MixerItem
                        android:id="@+id/id_mixer_13"
                        android:layout_width="match_parent"
                        android:layout_height="95dp"/>
                    <com.chs.mt.pxe_r500.viewItem.V_MixerItem
                        android:id="@+id/id_mixer_14"
                        android:layout_width="match_parent"
                        android:layout_height="95dp"/>
                    <com.chs.mt.pxe_r500.viewItem.V_MixerItem
                        android:id="@+id/id_mixer_15"
                        android:layout_width="match_parent"
                        android:layout_height="95dp"/>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="10dp"
                        />
                </LinearLayout>
            </com.chs.mt.pxe_r500.tools.MScrollView>
        </LinearLayout>
        <Button
            android:id="@+id/id_b_encryption"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="@string/Encrying_data"
            android:textColor="@color/text_color_red"
            android:textSize="35sp"
            android:textAllCaps="false"
            android:visibility="gone"
            android:background="@drawable/chs_btn_encryption"/>


    </RelativeLayout>

</FrameLayout>
