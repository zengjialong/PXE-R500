<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:KNOB="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center" 
	android:gravity="center" 
    android:background="@null"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:gravity="center"
        android:orientation="horizontal">
        <!-- 通道号 -->
		<LinearLayout
			android:layout_weight="2.0"
			android:layout_width="0dp"
			android:layout_height="match_parent"
            android:layout_gravity="center"
            android:gravity="center"
            android:orientation="vertical"
			>
			<LinearLayout
				android:layout_weight="1.0"
				android:layout_width="match_parent"
				android:layout_height="0dp"
                android:layout_gravity="center"
                android:gravity="center"
                android:orientation="vertical"
				>
				<Button
					android:id="@+id/id_b_ch"
					android:layout_width="80dp"
					android:layout_height="28dp"
					android:layout_gravity="center"
					android:gravity="center"
					android:clickable="false"
					android:background="@null"
					android:maxLines="1"
					android:textColor="@color/xover_channel_text_color"
					android:text="CHS"
					android:textSize="13sp"
					android:textAllCaps="false"
					/>
			</LinearLayout>
            <View
                android:layout_weight="5.0"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                />
		</LinearLayout>
        <!--  高低通 -->
        <LinearLayout
            android:layout_weight="1.0"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:gravity="center"
            android:orientation="vertical"
            >
            <Button
                android:id="@+id/id_b_hp"
                android:layout_weight="1.0"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_gravity="center"
                android:gravity="center"
                android:clickable="false"
                android:background="@null"
                android:maxLines="1"
                android:textColor="@color/xover_hlpass_text_color"
                android:text="@string/HighPass"
                android:textSize="13sp"
                android:textAllCaps="false"
                />
            <View
                android:layout_weight="2.0"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                />
            <Button
                android:id="@+id/id_b_lp"
                android:layout_weight="1.0"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_gravity="center"
                android:gravity="center"
                android:clickable="false"
                android:background="@null"
                android:maxLines="1"
                android:textColor="@color/xover_hlpass_text_color"
                android:text="@string/LowPass"
                android:textSize="13sp"
                android:textAllCaps="false"
                />
            <View
                android:layout_weight="2.0"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                />
        </LinearLayout>

        <!--  高低通 -->
        <LinearLayout
            android:layout_weight="2.0"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:gravity="center"
            android:orientation="vertical"
            >
            <!--  高通 -->
            <LinearLayout
                android:layout_weight="1.0"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_gravity="center"
                android:gravity="center"
                android:orientation="vertical"
                >
                <Button
                    android:id="@+id/id_b_hp_filter"
                    android:layout_width="@dimen/filter_button_width"
                    android:layout_height="@dimen/filter_button_height"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:background="@null"
                    android:maxLines="1"
                    android:textColor="@color/xover_channel_text_color"
                    android:text="滤波器"
                    android:textSize="13sp"
                    android:textAllCaps="false"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_weight="1.0"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_gravity="center"
                android:gravity="center"
                android:orientation="vertical"
                >
                <Button
                    android:id="@+id/id_b_hp_level"
                    android:layout_width="@dimen/filter_button_width"
                    android:layout_height="@dimen/filter_button_height"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:background="@null"
                    android:maxLines="1"
                    android:textColor="@color/xover_channel_text_color"
                    android:text="滤波器"
                    android:textSize="13sp"
                    android:textAllCaps="false"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_weight="1.0"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_gravity="center"
                android:gravity="center"
                android:orientation="vertical"
                >
                <Button
                    android:id="@+id/id_b_hp_freq"
                    android:layout_width="@dimen/filter_button_width"
                    android:layout_height="@dimen/filter_button_height"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:background="@null"
                    android:maxLines="1"
                    android:textColor="@color/xover_channel_text_color"
                    android:text="滤波器"
                    android:textSize="13sp"
                    android:textAllCaps="false"
                    />
            </LinearLayout>

            <!--  低通 -->
            <LinearLayout
                android:layout_weight="1.0"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_gravity="center"
                android:gravity="center"
                android:orientation="vertical"
                >
                <Button
                    android:id="@+id/id_b_lp_filter"
                    android:layout_width="@dimen/filter_button_width"
                    android:layout_height="@dimen/filter_button_height"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:background="@null"
                    android:maxLines="1"
                    android:textColor="@color/xover_channel_text_color"
                    android:text="滤波器"
                    android:textSize="13sp"
                    android:textAllCaps="false"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_weight="1.0"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_gravity="center"
                android:gravity="center"
                android:orientation="vertical"
                >
                <Button
                    android:id="@+id/id_b_lp_level"
                    android:layout_width="@dimen/filter_button_width"
                    android:layout_height="@dimen/filter_button_height"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:background="@null"
                    android:maxLines="1"
                    android:textColor="@color/xover_channel_text_color"
                    android:text="滤波器"
                    android:textSize="13sp"
                    android:textAllCaps="false"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_weight="1.0"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_gravity="center"
                android:gravity="center"
                android:orientation="vertical"
                >
                <Button
                    android:id="@+id/id_b_lp_freq"
                    android:layout_width="@dimen/filter_button_width"
                    android:layout_height="@dimen/filter_button_height"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:background="@null"
                    android:maxLines="1"
                    android:textColor="@color/xover_channel_text_color"
                    android:text="滤波器"
                    android:textSize="13sp"
                    android:textAllCaps="false"
                    />
            </LinearLayout>
        </LinearLayout>
	</LinearLayout>
</RelativeLayout>